@model IEnumerable<Category>

<div class="list-group">

    <a asp-controller="Home" asp-action="Index" class="list-group-item list-group-item-action">Tüm Kategoriler</a>

    @foreach (var category in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@category.Id" class="list-group-item list-group-item-action">@category.Name</a>
    }    
   
    
  
</div>

